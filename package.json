{
  "name": "react-async-button",
  "version": "0.0.3",
  "description": "React button component for handling async actions ",
  "main": "lib/main.js",
  "scripts": {
    "clean": "rimraf lib",
    "build:commonjs": "babel src/ --out-dir lib/ --ignore __tests__,__mocks__",
    "build": "npm run clean && npm run build:commonjs",
    "lint": "eslint src",
    "test": "npm run lint && jest",
    "prepublish": "npm test && npm run build"
  },
  "files": [
    "lib",
    "src"
  ],
  "keywords": [
    "react",
    "async-button",
    "react-component"
  ],
  "repository": "https://github.com/selvagsz/react-async-button",
  "bugs": {
    "url": "https://github.com/selvagsz/react-async-button/issues"
  },
  "author": "selvagsz",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.0.4",
    "babel-jest": "^13.0.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "enzyme": "^2.3.0",
    "eslint": "^2.13.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.9.2",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-react": "^5.2.2",
    "jest-cli": "^13.2.1",
    "react": "^15.1.0",
    "react-addons-test-utils": "^15.1.0",
    "react-dom": "^15.1.0",
    "rimraf": "^2.5.2",
    "sinon": "^1.17.4"
  },
  "dependencies": {
    "classnames": "^2.2.5"
  },
  "jest": {
    "automock": false,
    "testPathDirs": [
      "src"
    ],
    "unmockedModulePathPatterns": [
      "node_modules",
      "babel"
    ]
  }
}
